# Backend Environment Variables for QA Environment (AWS)

# Environment
NODE_ENV=production
BACKEND_PORT=3001

# AWS Configuration
AWS_REGION=us-east-1
# AWS_ENDPOINT_URL is not set for real AWS
# AWS_ACCESS_KEY_ID and AWS_SECRET_ACCESS_KEY should be set via AWS CLI or IAM roles

# DynamoDB Configuration
TABLE_NAME=qa-ebook-platform-42611dbe
# DYNAMODB_ENDPOINT is not set for real AWS

# JWT Configuration (QA environment keys)
JWT_PUBLIC_KEY=qa-ebook-platform-jwt-public-key-2024-secure
JWT_PRIVATE_KEY=qa-ebook-platform-jwt-private-key-2024-secure
JWT_ACCESS_EXPIRY=15m
JWT_REFRESH_EXPIRY=7d

# Encryption (QA environment key - 32 characters)
ENCRYPTION_KEY=qa-ebook-platform-encrypt-key-32c

# CORS Configuration
CORS_ORIGIN=https://d2xg2iv1qaydac.cloudfront.net,http://localhost:3000

# Logging
LOG_LEVEL=info

# S3 Configuration (will be populated by Terraform outputs)
ASSETS_BUCKET=qa-ebook-assets-96c175f3
FRONTEND_BUCKET=qa-ebook-frontend-96c175f3

# SNS/SQS Configuration (will be populated by Terraform outputs)
NOTIFICATION_TOPIC_ARN=arn:aws:sns:us-east-1:ACCOUNT_ID:qa-user-notifications
WORKFLOW_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/ACCOUNT_ID/qa-book-workflow-queue

# Email Configuration (SES)
SES_REGION=us-east-1
FROM_EMAIL=noreply@qa.example.com